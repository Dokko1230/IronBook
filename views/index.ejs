<div class="container main">
  <div class="row">
    <div class="twelve columns">
      <form method="post" action="/lift">
        Name<input name='name' type="text">
        Weight<input name='weight' type="text">
        Reps<input name='reps' type="text">
        Sets<input name='sets' type="text">
        <input type="submit">
      </form>
    </div>
  </div>
</div>

<div class="container lifts">
  <div class="row">
    
  </div>
</div>


<script id='liftTemplate' type='text/x-handlebars-template'>
  <div class='lift'>
    <div class='liftName'> {{ name }} </div>
    <div class='liftWeight'> {{ weight }} </div>
    <div class='liftReps'> {{ reps }} </div>
    <div class='liftSets'> {{ sets }} </div>
    <button>PR!</button>
  </div>
</script>


